{
  "metadata": [{
    "src": [{
      "files": [ 
        "EntityFramework/EntityFramework.sln"
      ],
      "src": "../src"
    }],
    "dest": "."
  }],
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "ef/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "ef/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "ef/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/ef/core/api/breadcrumb.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "ef"
  }
}
